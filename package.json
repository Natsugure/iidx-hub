{
  "name": "iidx-hub",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --parallel --filter @iidx-hub/backend --filter @iidx-hub/frontend dev",
    "dev:scraper": "pnpm --filter @iidx-hub/scraper dev",
    "scrape:now": "pnpm --filter @iidx-hub/scraper scrape:now",
    "db:migrate": "pnpm --filter @iidx-hub/database db:migrate",
    "db:studio": "pnpm --filter @iidx-hub/database db:studio",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "keywords": [],
  "author": "Natsugure",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "axios": "^1.6.0"
  }
}
